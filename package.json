    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-babel-istanbul": "^1.6.0",
    "gulp-esdoc": "^0.4.1",
    "gulp-eslint": "^4.0.0",
    "gulp-inject-modules": "^1.0.0",
    "gulp-mocha": "^3.0.1",
